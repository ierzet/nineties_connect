    "version": "0.2.0",
    "configurations": [

        {
            "name": "nineties_connect",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "nineties_connect (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "nineties_connect (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "nineties_connect (web with canvaskit renderer)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "-d",
                "chrome",
                "--web-renderer",
                "canvaskit"
            ]
        }
    ]
}
